{
  "name": "my-dapp-template",
  "version": "0.1.0",
  "license": "GPL",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run clean && npx --yes tsc --build .",
    "start": "next start",
    "clean": "npm run clean:windows && npm run clean:unix",
    "clean:unix": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf dist",
    "clean:windows": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , if exist dist rmdir /Q /S dist",
    "compile": "npx cli compile",
    "deploy": "npx cli deploy",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --fix --ext ts",
    "test": "jest -i --config ./jest-config.json"
  },
  "dependencies": {
    "@alephium/cli": "^1.7.3",
    "@alephium/sdk": "^0.7.6",
    "@alephium/web3": "^1.7.3",
    "@alephium/web3-react": "^1.7.3",
    "@alephium/web3-test": "^1.7.3",
    "@alephium/web3-wallet": "^1.7.3",
    "next": "14.2.11",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^20",
    "@types/react": "18.3.5",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-next": "14.2.11",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "bracketSameLine": false,
    "trailingComma": "none"
  }
}
